package siagsce.viewmodel.transacciones;

import siagsce.modelo.data.maestros.Actividad;

public class VMActividadEstatus {

	private boolean estatus;
	private Actividad actividad;
	
	public boolean isEstatus() {
		return estatus;
	}

	public void setEstatus(boolean estatus) {
		this.estatus = estatus;
	}

	public Actividad getActividad() {
		return actividad;
	}

	public void setActividad(Actividad actividad) {
		this.actividad = actividad;
	}

	public VMActividadEstatus() {
	
	}

	public VMActividadEstatus(boolean estatus, Actividad actividad) {
		super();
		this.estatus = estatus;
		this.actividad = actividad;
	}

}
