/** @Modelo Actividades Estudiante - Reporte Especial
 * Modelo usado en la generación del reporte Actividades
 * asignadas a estudiantes en el SCE
 * @author Iterator
 */

package siagsce.modelo.data.reportes.especiales;

import java.util.Date;

/**
 * Declaración de variables.
 */

public class ActividadesEstudiante {
	private Date  fechaInicio;
	private Date fechaCulminacion;
	private String actividadNombre;
	private String actividadDescripcion;
	private String proyectoCodigo;
	private String proyectoNombre;
	private Date fechaActividadEjecutada;
	private String actividadEjecutadaNombre;
	private String actividadEjecutadaDescripcion;
	private String proyectoCodigoActividadEjecutada;
	private String proyectoNombreActividadEjecutada;
	private Integer actividadEjecutadaHoras;
	
	
	/**
	 * Declaración del constructor
	 */
	public ActividadesEstudiante(Date fechaInicio,
			Date fechaCulminacion, String actividadNombre,
			String actividadDescripcion, String proyectoCodigo,
			String proyectoNombre) {
		super();
		this.fechaInicio = fechaInicio;
		this.fechaCulminacion = fechaCulminacion;
		this.actividadNombre = actividadNombre;
		this.actividadDescripcion = actividadDescripcion;
		this.proyectoCodigo = proyectoCodigo;
		this.proyectoNombre = proyectoNombre;
	}
	

	/**
	 * Declaración del constructor
	 */
	public ActividadesEstudiante(String actividadEjecutadaNombre,
			String actividadEjecutadaDescripcion,
			String proyectoCodigoActividadEjecutada,
			String proyectoNombreActividadEjecutada,
			Date fechaActividadEjecutada,
			Integer actividadEjecutadaHoras) {
		super();
		this.actividadEjecutadaNombre = actividadEjecutadaNombre;
		this.actividadEjecutadaDescripcion = actividadEjecutadaDescripcion;
		this.proyectoCodigoActividadEjecutada = proyectoCodigoActividadEjecutada;
		this.proyectoNombreActividadEjecutada = proyectoNombreActividadEjecutada;
		this.fechaActividadEjecutada = fechaActividadEjecutada;
		this.actividadEjecutadaHoras = actividadEjecutadaHoras;
	}


	/**
	 * Declaración de set y get
	 */
	public Date getFechaInicio() {
		return fechaInicio;
	}

	public void setFechaInicio(Date fechaInicio) {
		this.fechaInicio = fechaInicio;
	}

	public Date getFechaCulminacion() {
		return fechaCulminacion;
	}

	public void setFechaCulminacion(Date fechaCulminacion) {
		this.fechaCulminacion = fechaCulminacion;
	}

	public String getActividadNombre() {
		return actividadNombre;
	}

	public void setActividadNombre(String actividadNombre) {
		this.actividadNombre = actividadNombre;
	}

	public String getActividadDescripcion() {
		return actividadDescripcion;
	}

	public void setActividadDescripcion(String actividadDescripcion) {
		this.actividadDescripcion = actividadDescripcion;
	}

	public String getProyectoCodigo() {
		return proyectoCodigo;
	}

	public void setProyectoCodigo(String proyectoCodigo) {
		this.proyectoCodigo = proyectoCodigo;
	}

	public String getProyectoNombre() {
		return proyectoNombre;
	}

	public void setProyectoNombre(String proyectoNombre) {
		this.proyectoNombre = proyectoNombre;
	}



	public Date getFechaActividadEjecutada() {
		return fechaActividadEjecutada;
	}



	public void setFechaActividadEjecutada(Date fechaActividadEjecutada) {
		this.fechaActividadEjecutada = fechaActividadEjecutada;
	}



	public String getActividadEjecutadaNombre() {
		return actividadEjecutadaNombre;
	}



	public void setActividadEjecutadaNombre(String actividadEjecutadaNombre) {
		this.actividadEjecutadaNombre = actividadEjecutadaNombre;
	}



	public String getActividadEjecutadaDescripcion() {
		return actividadEjecutadaDescripcion;
	}



	public void setActividadEjecutadaDescripcion(
			String actividadEjecutadaDescripcion) {
		this.actividadEjecutadaDescripcion = actividadEjecutadaDescripcion;
	}



	public String getProyectoCodigoActividadEjecutada() {
		return proyectoCodigoActividadEjecutada;
	}



	public void setProyectoCodigoActividadEjecutada(
			String proyectoCodigoActividadEjecutada) {
		this.proyectoCodigoActividadEjecutada = proyectoCodigoActividadEjecutada;
	}



	public String getProyectoNombreActividadEjecutada() {
		return proyectoNombreActividadEjecutada;
	}



	public void setProyectoNombreActividadEjecutada(
			String proyectoNombreActividadEjecutada) {
		this.proyectoNombreActividadEjecutada = proyectoNombreActividadEjecutada;
	}


	public Integer getActividadEjecutadaHoras() {
		return actividadEjecutadaHoras;
	}


	public void setActividadEjecutadaHoras(Integer actividadEjecutadaHoras) {
		this.actividadEjecutadaHoras = actividadEjecutadaHoras;
	}

	
}
