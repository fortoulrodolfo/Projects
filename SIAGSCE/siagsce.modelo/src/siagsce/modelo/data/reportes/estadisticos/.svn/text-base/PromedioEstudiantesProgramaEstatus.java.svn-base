/** @Modelo Listado PromedioEstudiantesProgramaEstatus - Reporte Estadístico
 * Modelo usado en la generación del reporte de promedio de estudiantes
 * en las diferentes direcciones de programa de acuerdo a los estatus en SCE
 * @author Iterator
 */

package siagsce.modelo.data.reportes.estadisticos;

import java.math.BigDecimal;
import java.math.BigInteger;


/**
 * Declaración de variables.
 */
public class PromedioEstudiantesProgramaEstatus {
	
	private String nombrePrograma;
	private BigInteger cantidadAptos;
	private BigInteger cantidadAcreditados;
	private BigInteger cantidadAprobados;
	private BigInteger cantidadTotalEstudiantes;
	
	
	/**
	 * Declaración del constructor
	 */
	public PromedioEstudiantesProgramaEstatus() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public PromedioEstudiantesProgramaEstatus(String nombrePrograma,
			BigInteger cantidadAptos,
			BigInteger cantidadAcreditados, BigInteger cantidadAprobados, BigInteger cantidadTotalEstudiantes) {
		super();
		this.nombrePrograma = nombrePrograma;
		this.cantidadAptos = cantidadAptos;
		this.cantidadAcreditados = cantidadAcreditados;
		this.cantidadAprobados = cantidadAprobados;
		this.cantidadTotalEstudiantes  = cantidadTotalEstudiantes;
		
	}


	/**
	 * Declaración de set y get
	 */
	public String getNombrePrograma() {
		return nombrePrograma;
	}


	public void setNombrePrograma(String nombrePrograma) {
		this.nombrePrograma = nombrePrograma;
	}


	public BigInteger getCantidadAptos() {
		return cantidadAptos;
	}


	public void setCantidadAptos(BigInteger cantidadAptos) {
		this.cantidadAptos = cantidadAptos;
	}


	public BigInteger getCantidadAcreditados() {
		return cantidadAcreditados;
	}


	public void setCantidadAcreditados(BigInteger cantidadAcreditados) {
		this.cantidadAcreditados = cantidadAcreditados;
	}


	public BigInteger getCantidadAprobados() {
		return cantidadAprobados;
	}


	public void setCantidadExonerados(BigInteger cantidadAprobados) {
		this.cantidadAprobados = cantidadAprobados;
	}
	
	public void setCantidadTotalEstudiantes(BigInteger cantidadTotalEstudiantes){
		this.cantidadTotalEstudiantes = cantidadTotalEstudiantes;
	}

	public BigInteger getCantidadTotalEstudiantes(){
		return cantidadTotalEstudiantes;
	}


	
}
